import java.util.*;
public class UnionOf2Arrays {
    public static void main(String[] args) {
        int[] arr1 = {5,10,15,5};
        int[] arr2 = {10,15,4};
        HashSet<Integer> s = new HashSet<>();
        for(int i : arr1){
            s.add(i);
        }
        for(int j: arr2){
            s.add(j);
        }
        System.out.println(s.size());
        System.out.println(s);
    }
}
