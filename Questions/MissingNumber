//https://practice.geeksforgeeks.org/problems/missing-number-in-array1416/1?page=1&sprint=a663236c31453b969852f9ea22507634&sortBy=submissions

class Solution {
    int missingNumber(int array[], int n) {
        // Your Code Here
        int total;
        total = (n+1)*(n)/2;
        for(int i=0; i<n-1; i++){
            total-=array[i];
        }
        return total;
    }
}
