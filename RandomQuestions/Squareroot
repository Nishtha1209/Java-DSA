//https://leetcode.com/problems/sqrtx/

class Solution {
    public int mySqrt(int x) {
        double t;
        double sqrt = x/2;
        do{
            t = sqrt;
            sqrt = (t+(x/t))/2;
        }
        while (t-sqrt!=0);
        return (int)(sqrt);
    }
}
//TIME COMPLEXITY CONSTRAINTS-- NOT AN OPTIMAL SOLUTION
